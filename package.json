{
  "name": "@funnyzak/aliyun-nls",
  "version": "0.1.2",
  "description": "a aliyun nls node module.",
  "main": "index.js",
  "files": [
    "lib/**/*",
    "index.js"
  ],
  "types": "lib/nls.d.ts",
  "scripts": {
    "lint": "eslint --fix src",
    "test": "echo \"o test specified\"",
    "dev": "watch \"npm run build\" src",
    "build": "babel src -d lib",
    "prepublish": "npm run build",
    "release": "np",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/funnyzak/aliyun-nls.git"
  },
  "keywords": [
    "aliyun nls",
    "nls",
    "aliyun",
    "nodejs",
    "funnyzak"
  ],
  "author": "eric <silenceace@gmail.com>",
  "license": "Apache 2.0",
  "bugs": {
    "url": "https://github.com/funnyzak/aliyun-nls/issues"
  },
  "homepage": "https://github.com/funnyzak/aliyun-nls#readme",
  "dependencies": {
    "@alicloud/pop-core": "1.7.10",
    "chalk": "4.1.2",
    "request-promise": "4.2.6"
  },
  "devDependencies": {
    "@babel/cli": "7.16.0",
    "@babel/core": "7.16.0",
    "@babel/plugin-transform-runtime": "7.16.4",
    "@babel/preset-env": "7.16.4",
    "core-js": "3.19.1",
    "eslint": "8.2.0",
    "husky": "7.0.4",
    "lint-staged": "12.0.3",
    "np": "*",
    "prettier": "2.4.1",
    "typescript": "4.5.2",
    "watch": "1.0.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "entry",
          "corejs": "3.3.5"
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "np": {
    "yarn": false,
    "contents": ".",
    "branch": "master",
    "releaseDraft": false
  },
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "*.{js}": "eslint --cache --fix",
    "*.{js,ts,css,md}": "prettier --write"
  }
}
